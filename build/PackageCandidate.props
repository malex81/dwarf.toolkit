<Project>
	<PropertyGroup>
		<Company>Dwarf holl</Company>
		<Copyright>Copyright Dwarf holl © 2025-2026</Copyright>
		<MajorVersion>0.1</MajorVersion>
		<SignAssembly>True</SignAssembly>
		<AssemblyOriginatorKeyFile>$(MSBuildThisFileDirectory)..\keys\Dwarf.snk</AssemblyOriginatorKeyFile>
		<DebugType>portable</DebugType>
		<!-- Старый вариант - собирает pdb и dll в один большой пакет -->
		<!--<IncludeSymbolsInPackage>true</IncludeSymbolsInPackage>
		<AllowedOutputExtensionsInPackageBuildOutputFolder>$(AllowedOutputExtensionsInPackageBuildOutputFolder);.pdb</AllowedOutputExtensionsInPackageBuildOutputFolder>-->
		<!-- Современный вариант -->
		<IncludeSymbols>true</IncludeSymbols>
		<SymbolPackageFormat>snupkg</SymbolPackageFormat>
		<EmbedAllSources>true</EmbedAllSources>
		<ManagePackageVersionsCentrally>true</ManagePackageVersionsCentrally>
	</PropertyGroup>

	<Import Project="Netstandard.props" />
</Project>